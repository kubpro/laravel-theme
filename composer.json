{
    "name": "kubpro/theme",
    "description": "Laravel multi Themes Package",
    "keywords": ["theme", "views", "assets", "blade", "laravel-5", "package"],
    "license": "MIT",
    "homepage": "https://github.com/kubpro/laravel-theme.git",
    "authors": [
        {
            "name": "Rauf Abbas",
            "email": "rafo.abbas@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "^5.6",
        "php": "^7.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Kubpro\\Theme\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kubpro\\Theme\\Providers\\ThemeServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "version": "1.2.0"
}
